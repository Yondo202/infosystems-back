{
  "kind": "collectionType",
  "collectionName": "issue_answers",
  "info": {
    "name": "issue-answer"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "product_issue": {
      "model": "product-feedback",
      "via": "issue_answers"
    },
    "user": {
      "via": "issue_answers",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
